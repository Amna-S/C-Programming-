/*#include <iostream>
int main()
{
    int x = 7;
    int y = 9;
    std::cout<<(x==y)<<"\n";//equals to
    std::cout<<(x != y)<<"\n";//not equal to
    std::cout<<(x > y)<<"\n";//greater than
    std::cout<<(x < y)<<"\n";//less than
