//logical operators:
//&& logical and:(returns true if both statements are true)
/*#include <iostream>
int main()
{
    int a=9;
    int b=6;
    std::cout<<(a>b && b<a);
    return 0;
}*/
//|| logical or:(returns true if one statement is true)
/*#include <iostream>
int main()
{
    int a=9;
    int b=6;
    std::cout<<(a>b || a<b);
    return 0;
}*/
//! logical not:(reverse the result, will return false if result is true)
/*#include <iostream>
int main()
{
    int a=9;
    int b=6;
    std::cout<<(!(a>b && b<a));
    return 0;
}*/
